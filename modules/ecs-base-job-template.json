{
  "job_configuration": {
    "command": "{{ command }}",
    "env": "{{ env }}",
    "labels": "{{ labels }}",
    "name": "{{ name }}",
    "aws_credentials": {"allOf": [
      {
        "$ref": "#/definitions/AwsCredentials"
      }
    ],
    "default": {
      "$ref": {
        "block_document_id": "f214a553-b4c9-4e97-a6c8-4078956084bd"
      }
    }},
    "task_definition": {
      "containerDefinitions": [
        {
          "image": "{{ image }}",
          "name": "{{ container_name }}"
        }
      ],
      "cpu": "{{ cpu }}",
      "family": "{{ family }}",
      "memory": "{{ memory }}",
      "executionRoleArn": "{{ execution_role_arn }}"
    },
    "task_run_request": {
      "launchType": "FARGATE",
      "cluster": "arn:aws:ecs:us-east-1:790198870643:cluster/run-prefect-job-prod-cluster",
      "overrides": {
        "containerOverrides": [
          {
            "name": "{{ container_name }}",
            "command": "{{ command }}",
            "environment": "{{ env }}",
            "cpu": "{{ cpu }}",
            "memory": "{{ memory }}"
          }
        ],
        "cpu": "{{ cpu }}",
        "memory": "{{ memory }}",
        "taskRoleArn": "{{ task_role_arn }}"
      },
      "tags": "{{ labels }}",
      "taskDefinition": "arn:aws:ecs:us-east-1:790198870643:task-definition/run-prefect-job:1"
    },
    "configure_cloudwatch_logs": "{{ configure_cloudwatch_logs }}",
    "cloudwatch_logs_options": "{{ cloudwatch_logs_options }}",
    "task_start_timeout_seconds": "{{ task_start_timeout_seconds }}",
    "auto_deregister_task_definition": "{{ auto_deregister_task_definition }}",
    "vpc_id": "{{ vpc_id }}",
    "container_name": "{{ container_name }}",
    "cluster": "{{ cluster }}"
  }
}
